package leetCode;

public class SurroundedRegion {
	public void solve(char[][] board) {
		for (int j=0;j<board[0].length;j++){
			if (board[0][j]=='0'){
				board[0][j] = 'Y';
				checkNeighbor(board[0][j]);
			}
			if (board[board.length-1][j]=='0'){
				board[board.length-1][j] = 'Y';
				checkNeighbor(board[0][j]);
			}
		}
	}
	
	public void checkNeighbor
}
